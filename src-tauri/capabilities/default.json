{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main",
    "emulator",
    "settings",
    "about"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "sql:default",
    "sql:allow-execute",
    "dialog:default",
    "dialog:allow-open",
    "core:window:allow-set-size",
    "core:window:allow-destroy",
    "fs:allow-copy-file"
  ]
}